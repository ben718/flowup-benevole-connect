{
    "name":  "voisin-solidaire",
    "private":  true,
    "version":  "0.0.0",
    "scripts":  {
                    "dev":  "vite",
                    "build":  "vite build",
                    "build:full":  "powershell -ExecutionPolicy Bypass -File build.ps1",
                    "build:prod":  "powershell -ExecutionPolicy Bypass -File build.ps1 -Production",
                    "build:netlify":  "powershell -ExecutionPolicy Bypass -File build.ps1 -Production -DeployToNetlify",
                    "build:vercel":  "powershell -ExecutionPolicy Bypass -File build.ps1 -Production -DeployToVercel",
                    "build:quick":  "powershell -ExecutionPolicy Bypass -File build.ps1 -SkipTests",
                    "lint":  "eslint .",
                    "preview":  "vite preview",
                    "preview:network":  "vite preview --host",
                    "deploy":  "npm run build \u0026\u0026 npx gh-pages -d dist",
                    "test":  "vitest run",
                    "test:watch":  "vitest",
                    "test:coverage":  "vitest run --coverage",
                    "test:fix":  "powershell -ExecutionPolicy Bypass -File run-tests.ps1"
                },
    "dependencies":  {
                         "@sentry/browser":  "^9.29.0",
                         "@sentry/react":  "^9.29.0",
                         "@supabase/supabase-js":  "^2.50.0",
                         "lucide-react":  "^0.514.0",
                         "react":  "^19.1.0",
                         "react-dom":  "^19.1.0",
                         "react-router-dom":  "^7.6.2"
                     },
    "devDependencies":  {
                            "@babel/preset-env":  "^7.27.2",
                            "@babel/preset-react":  "^7.27.1",
                            "@eslint/js":  "^9.25.0",
                            "@testing-library/jest-dom":  "^6.6.3",
                            "@testing-library/react":  "^16.3.0",
                            "@types/react":  "^19.1.2",
                            "@types/react-dom":  "^19.1.2",
                            "@vitejs/plugin-react":  "^4.4.1",
                            "@vitest/coverage-v8":  "^3.2.3",
                            "autoprefixer":  "^10.4.21",
                            "eslint":  "^9.25.0",
                            "eslint-plugin-react-hooks":  "^5.2.0",
                            "eslint-plugin-react-refresh":  "^0.4.19",
                            "globals":  "^16.0.0",
                            "jest":  "^30.0.0",
                            "jest-environment-jsdom":  "^30.0.0",
                            "jsdom":  "^26.1.0",
                            "postcss":  "^8.5.5",
                            "tailwindcss":  "^3.4.17",
                            "typescript":  "~5.8.3",
                            "typescript-eslint":  "^8.30.1",
                            "vite":  "^6.3.5",
                            "vitest":  "^3.2.3"
                        },
    "type":  "module"
}
